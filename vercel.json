{
  "rewrites": [
    {
      "source": "/api/beehiv-signup",
      "destination": "/api/beehiv-signup.js"
    }
  ],
  "headers": [
    {
      "source": "/.well-known/apple-app-site-association",
      "headers": [
        { "key": "Content-Type", "value": "application/json" },
        { "key": "Cache-Control", "value": "public, max-age=300" }
      ]
    }
  ]
}
